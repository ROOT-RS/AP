  'ajs'
], function (a) {
  var b;
  String._as = {
    '\'': new RegExp('(^|[^\\\\])((\\\\)*)\'', 'g'),
    '"': new RegExp('(^|[^\\\\])((\\\\)*)"', 'g')
  };
  window.defined = window.defined || function (a, b) {
    return typeof b == 'undefined' ? typeof a != 'undefined' : typeof a == 'undefined' ? b : a
  };
  var c = {
    _hex: function (a) {
      return a.length < 2 ? a + '' + a : a
    },
    rgb2hex: function (a) {
      if (typeof a == 'string') {
        if (a.indexOf(',') < 0) return '#' + a.replace('#', '').replace(/^(\w)(\w)(\w)$/gi, '$1$1$2$2$3$3');
        a = a.replace(/(rgb\(|\)|\s+)/gi, '').split(',')
      } else if (a.red) {
        a[0] = a.red;
        a[1] = a.green;
        a[2] = a.blue
      }
      a = String._hex((a[0] * 1).toString(16)) + String._hex((a[1] * 1).toString(16)) + String._hex((a[2] * 1).toString(16));
      return '#' + a
    },
    addSlashes: function (a, b) {
      return String(a).replace(String._as[b = b || '\''], '$1$2\\' + b)
    },
    padZero: function (a, b) {
      var c = '' + a;
      while (c.length < b) c = '0' + c;
      return c
    },
    trim: function (a) {
      return String(a).replace(/^([\s\r\n]+)|([\s\r\n]+)$/g, '')
    },
    num: function (a, b, c) {
      if (b == null) return '';
      var d = a % 100 <= 10 || a % 100 >= 20 ? a % 10 : 0;
      var e = b.length;
      if (e == 2 && d == 1) d = 0;
       else if (e == 3 && d > 1) {
        d = d < 5 ? 2 : 0
      }
      return (c ? a + (c && c != 1 ? c : '')  : '') + (typeof b != 'string' ? b[d] || b[d > 1 && d < 5 ? d : 0] || b[d > 1 ? 1 : 0] : b)
    },
    ucfirst: function (a) {
      return a.charAt(0).toUpperCase() + a.substr(1)
    },
    repeat: function (a, b) {
      var c = '',
      d = 0;
      for (; d < b; d++) c += a;
      return c
    },
    toQuery: function (a) {
      var b = '',
      c = 0,
      d,
      e,
      f;
      if (a) {
        if (typeof a != 'string') {
          b = [
          ];
          for (var g in a) if (g != '') {
            g = encodeURIComponent(g);
            f = defined(a[g], '');
            if (f && f.constructor === Array) {
              for (d = 0, e = f.length; d < e; d++) {
                b[c++] = g + '=' + encodeURIComponent(defined(f[d], ''))
              }
            } else {
              b[c++] = g + (f != null && f !== '' ? '=' + encodeURIComponent(f)  : '')
            }
          }
          b = b.join('&')
        } else b = a
      }
      return b
    },
    toObject: function (a) {
      var b = {
      };
      if (typeof a === 'object') {
        b = a || b
      } else if (a = String(a)) {
        if (a.indexOf('?') > - 1 || /^http:/.test(a)) a = a.split('?') [1] || '';
        a = a.split('&');
        for (var c = 0, d = a.length, e, f; c < d; c++) if (a[c] != '') {
          a[c] = a[c].split('=');
          e = a[c][1];
          f = a[c][0];
          if (typeof e == 'string') try {
            e = decodeURIComponent(e)
          } catch (a) {
            e = unescape(e)
          } else e = '';
          try {
            f = decodeURIComponent(f)
          } catch (a) {
            f = unescape(f)
          }
          b[f] = e
        }
        return b
      }
      return b
    },
    html2text: function (a) {
      return String(a).replace(/&/g, '&amp;').replace(/</g, '&lt;').replace(/>/g, '&gt;').replace(/"/g, '&quot;')
    },
    nl2br: function (a) {
      return String(a).replace(/[\r\n]/g, '<br />')
    },
    wordWrap: function (a, b, c, d, e) {
      a = String(a).replace(new RegExp('(\\S{' + (b || 20) + '})', 'ig'), '$1 ');
      if (c && a.length > c) {
        if (e) {
          for (c--; c < a.length; c++) if (/\s/.test(a.charAt(c))) {
            break
          }
        } else {
          for (c++; c--; ) if (/\s/.test(a.charAt(c))) {
            break
          }
        }
        a = a.substr(0, c) + d
      }
      return a
    },
    supplant: function (a, b) {
      return a.replace(/#\{([^\{\}]*)\}/g, function (a, c) {
        var d = b[c];
        return typeof d === 'string' || typeof d === 'number' ? d : a
      })
    },
    numberFormat: function (a, b, c, d) {
      var a = Math.round(a, b).toString().split('.');
      if (d) for (var e = a[0].length - 3; e >= 0; e -= 3) a[0] = a[0].substr(0, e) + d + a[0].substr(e);
      return a[0] + (a[1] ? c + a[1] : '')
    },
    sizeFormat: function (a) {
      if (a >= Math.TB) {
        a = String.numberFormat(a / Math.TB, 2, '.', '') + ' ' + Lang.get('Size').tb
      } else {
        if (a >= Math.GB) {
          a = String.numberFormat(a / Math.GB, 2, '.', '') + ' ' + Lang.get('Size').gb
        } else {
          if (a >= Math.MB) {
            a = String.numberFormat(a / Math.MB, 2, '.', '') + ' ' + Lang.get('Size').mb
          } else {
            if (a >= Math.KB) {
              a = String.numberFormat(a / Math.KB, 0) + ' ' + Lang.get('Size').kb
            } else {
              a = String.numberFormat(a, 0) + ' ' + Lang.get('Size').bytes
            }
          }
        }
      }
      return a
    },
    sprintf: function (a) {
      if (typeof a === 'string') {
        for (var b = 1, c = arguments.length; b < c; b++) a = a.replace('%s', arguments[b])
      }
      return a
    },
    concat: function () {
      return Array.prototype.join.call(arguments, '')
    },
    preg_quote: function (a) {
      return (a || '').replace(/[-[\]{}()*+?.,\\^$|#]/g, '\\$&')
    }
  };
  for (var d in c) {
    String[d] = c[d]
  }
  var e = String.prototype.split,
  f;
  if ('te'.split(/(s)*/) [1] != void 0 || '1_1'.split(/(_)/).length != 3) {
    f = /()??/.exec('') [1] === void 0;
    String.prototype.split = function (a, b) {
      var c = this;
      if (!(a instanceof RegExp)) {
