        if (!this.data[a[d] = a[d].toUpperCase()]) {
          this.locals.push(a[d]);
          this.data[a[d]] = {
          }
        }
      }
      if (c = b(c, true)) this.load(false, c.constructor === Function ? c : 0);
      return this
    },
    setLocal: function (a) {
      this.def = a.toUpperCase();
      this.addLocal(a);
      return this
    },
    load: function (a, b) {
      if (a) {
        if (typeof a == 'string') a = a.split(',');
        for (var c = 0, d = a.length; c < d; c++) {
          if (!this.vars[a[c]]) {
            this.vars[a[c]] = 1
          }
        }
      }
      var e = 0,
      f = '';
      for (var a in this.vars) {
        for (var g = 0, h = this.locals.length; g < h; g++) {
          if (!this.data[this.locals[g]][a]) {
            this.data[this.locals[g]][a] = 1;
            if (!e) e = {
            };
            if (!e[this.locals[g]]) e[this.locals[g]] = [
            ];
            e[this.locals[g]].push(a)
          }
        }
      }
      if (b) {
        b()
      }
    },
    add: function (a) {
      var b = [
      ];
      for (var c in a) {
