define('ajs/__lang/ajs__lang', [
  'ajs/__string/ajs__string'
], function (a) {
  function b(a, b) {
    return typeof b == 'undefined' ? typeof a != 'undefined' : typeof a == 'undefined' ? b : a
  }
  var c = {
    data: {
    },
    vars: {
    },
    locals: [
    ],
    def: '',
    path: '',
    init: function () {
      this.setLocal(window['LANG_DEFAULT'] || 'RU');
      return this
    },
    setDataPath: function (a) {
      this.path = a
    },
    addLocal: function (a, c) {
      if (typeof a == 'string') a = a.split(',');
