            d.push.apply(d, j.slice(1))
          }
          l = j[0].length;
          h = k;
          if (d.length >= b) {
            break
          }
        }
        if (a.lastIndex === j.index) {
          a.lastIndex++
        }
      }
      if (h === c.length) {
        if (l || !a.test('')) {
          d.push('')
        }
      } else {
        d.push(c.slice(h))
      }
      return d.length > b ? d.slice(0, b)  : d
    }
  }
