return ''
}
if (typeof b == 'string') {
return d + b
}
e = a % 100 <= 10 || a % 100 >= 20 ? a % 10 : 0;
length = b.length;
if (length == 2 && e == 1) {
e = 0
} else if (length == 3 && e > 1) {
e = e < 5 ? 2 : 0
}
return d + (b[e] || b[e > 1 && e < 5 ? e : 0] || b[e > 1 ? 1 : 0])
};
return i18n
}
a.i18n = c();
if (typeof define === 'function') {
define('mrg-i18n-utils', [
], function () {
return a.i18n
})
}
}) (window);
