                  if (c === true) {
                    c = i[d]
                  } else if (i[d] !== true) {
                    e = f[0];
                    g.splice(j--, 0, e)
                  }
                  break
                }
              }
            }
          }
          if (c !== true) {
            if (c && a['throws']) {
              b = c(b)
            } else {
              try {
                b = c(b)
              } catch (a) {
                return {
                  state: 'parsererror',
                  error: c ? a : 'No conversion from ' + h + ' to ' + e
                }
              }
            }
          }
        }
        h = e
      }
    }
    return {
      state: 'success',
      data: b
    }
  }
