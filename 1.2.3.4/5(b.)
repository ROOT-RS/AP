      }
      var d = [
      ],
      g = (a['ignoreCase'] ? 'i' : '') + (a['multiline'] ? 'm' : '') + (a['extended'] ? 'x' : '') + (a['sticky'] ? 'y' : ''),
      h = 0,
      i,
      j,
      k,
      l;
      a = new RegExp(a.source, g + 'g');
      c += '';
      if (!f) {
        i = new RegExp('^' + a.source + '$(?!\\s)', g)
      }
      b = b === void 0 ? - 1 >>> 0 : b >>> 0;
      if (!b) {
        return []
      }
      while (j = a.exec(c)) {
        k = j.index + j[0].length;
        if (k > h) {
          d.push(c.slice(h, j.index));
