  function Db(a, b) {
    var c,
    d,
    e,
    f,
    g = a.dataTypes.slice(),
    h = g[0],
    i = {
    },
    j = 0;
    if (a.dataFilter) {
      b = a.dataFilter(b, a.dataType)
    }
    if (g[1]) {
      for (c in a.converters) {
        i[c.toLowerCase()] = a.converters[c]
      }
    }
    for (; e = g[++j]; ) {
      if (e !== '*') {
        if (h !== '*' && h !== e) {
          c = i[h + ' ' + e] || i['* ' + e];
          if (!c) {
            for (d in i) {
              f = d.split(' ');
              if (f[1] === e) {
                c = i[h + ' ' + f[0]] || i['* ' + f[0]];
