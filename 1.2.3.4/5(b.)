define('ajs/__date/ajs__date', [
  'ajs',
  'jquery',
  'ajs/__string/ajs__string',
  'ajs/__lang/ajs__lang'
], function (a, $) {
  function b() {
    return (new Date).getTime()
  }
  $.extend(Date, {
    is: function (a) {
      if (a) {
        return a.setTime || /(^\d+$|^((\d{1,4}[-:\s\/]?)+)$)/.test(a + '')
      }
      return !1
    },
    _timer: (new Date).getTime(),
    timeZone: window.timeZone || window.TIMEZONE || 4,
    getTime: function () {
      return this.changeNow().getNow().getTime()
    },
    getUnixtime: function () {
      return Math.round(this.getTime() / 1000)
    },
    setNow: function (a) {
      this._now_ = new Date(a);
      this._today_ = new Date(a);
      this._yesterday_ = new Date(a);
      this._today_.setHours(0);
      this._today_.setMinutes(0);
      this._yesterday_.setTime(this._today_.getTime());
      this._yesterday_.setDate(this._yesterday_.getDate() - 1);
      return this
    },
    getNow: function () {
      return this.changeNow()._now_
    },
    changeNow: function () {
      var a = Date.now();
      Date.setNow(Date._now_.getTime() + (a - Date._timer));
      Date._timer = a;
      return this
    },
    _f: function (a) {
      return a > 9 ? a : '0' + a
    },
    _parse: Date.parse,
    parse: function (a) {
      if (a && a.setTime) return a;
      try {
        var b = /^\d+$/.test(a) ? new Date(a)  : Date._parse(a);
        if (b.constructor != Date) b = new Date(String(a).replace(/^(\d{4})-(\d{2})-(\d{2})/, '$2/$3/$1'))
      } catch (a) {
        debug.log('Date', a)
      }
      return b
    }
  });
  if (!Date.now) {
    Date.now = function () {
      return (new Date).getTime()
    }
  }
  $.extend(Date.prototype, {
    getLocalToday: function () {
      var a = new Date;
      a.setHours(0);
      a.setMinutes(0);
      a.setSeconds(0);
      return a
    },
    getLocalYesterday: function () {
      var a = this.getLocalToday();
      a.setDate(a.getDate() - 1);
      return a
    },
    isNow: function (a) {
      Date.changeNow();
      return Date._now_.getTime() - this.getTime() <= a
    },
    isToday: function () {
      return this.toDateString() === this.getLocalToday().toDateString()
    },
    isYesterday: function () {
      return this.toDateString() === this.getLocalYesterday().toDateString()
    },
    isThisYear: function () {
      return this.getFullYear() === this.getLocalToday().getFullYear()
    },
    setTimeZone: function (a) {
      this.timeZone = a * 1;
      return this
    },
    format: function (a) {
      var b = this.getTime(),
      c = '',
      a = a.split('');
      for (var d = 0, e = a.length; d < e; d++) {
        switch (a[d]) {
          case 'Y':
            c += this.getFullYear();
            break;
          case 'y':
            c += Date._f(this.getFullYear() % 100);
            break;
          case 'N':
            c += String.ucfirst(Lang.get('Date').months[1][this.getMonth()]);
            break;
          case 'n':
            c += Lang.get('Date').months[1][this.getMonth()];
            break;
          case 'M':
            c += Date._f(this.getMonth() + 1);
            break;
          case 'm':
            c += this.getMonth() + 1;
            break;
          case 'D':
            c += Date._f(this.getDate());
            break;
          case 'd':
            c += this.getDate();
            break;
          case 'H':
            c += Date._f(this.getHours());
            break;
          case 'h':
            c += this.getHours();
            break;
          case 'I':
            c += Date._f(this.getMinutes());
            break;
          case 'i':
            c += this.getMinutes();
            break;
          case 'S':
            c += Date._f(this.getSeconds());
            break;
          case 's':
            c += this.getSeconds();
            break;
          default:
            c += a[d];
            break
        }
      }
      this.setTime(b);
      return c
    },
    getLocaleDateShort: function () {
      var a = '';
      if (this.isToday()) {
        a = this.getLocaleTime()
      } else if (this.isThisYear()) {
        a = this.getLocaleShortDayMonth()
      } else {
        a = this.getLocaleShortDayMonthYear()
      }
      return a
    },
    getLocaleDateFull: function () {
      var a = '';
      if (this.isToday()) {
        a = this.getLocaleTodayTime()
      } else if (this.isYesterday()) {
        a = this.getLocaleYesterdayTime()
      } else if (this.isThisYear()) {
        a = this.getLocaleDayMonthTime()
      } else {
        a = this.getLocaleDayMonthYearTime()
      }
      return a
    },
    getLocaleFullDate: function () {
      var a = String.ucfirst(Lang.get('Date').weekdays_full[this.getDay()]);
      var b = - this.getTimezoneOffset(),
      c = '';
      if (b > 0) {
        c += '+'
      } else if (b < 0) {
        c += '-'
      }
      c += Date._f(b / 60) + ':' + Date._f(b % 60);
      if (patron.LANG == 'en_US') {
        return a + ', ' + this.format('n d, Y') + ' ' + this.getLocaleTime() + ' ' + c
      }
      return a + ', ' + this.format('d n Y') + ', ' + this.getLocaleTime() + ' ' + c
    },
    getLocaleTodayTime: function () {
      return String.ucfirst(Lang.get('Date').today) + ', ' + this.getLocaleTime()
    },
    getLocaleYesterdayTime: function () {
      return String.ucfirst(Lang.get('Date').yesterday) + ', ' + this.getLocaleTime()
    },
    getLocaleDayMonthTime: function () {
      if (patron.LANG == 'en_US') {
        return this.format('N d') + ', ' + this.getLocaleTime()
      }
      return this.format('d n') + ', ' + this.getLocaleTime()
    },
    getLocaleDayMonthYearTime: function () {
      if (patron.LANG == 'en_US') {
        return this.format('N d, Y') + ' ' + this.getLocaleTime()
      }
      return this.format('d n Y') + ', ' + this.getLocaleTime()
    },
    getLocaleTime: function () {
      if (patron.LANG == 'en_US') {
        return (this.getHours() % 12 || 12) + ':' + this.format('I') + ' ' + (this.getHours() < 12 ? 'AM' : 'PM')
      }
      return this.format('h:I')
    },
    getLocaleShortDayMonth: function () {
      var a = Lang.get('Date').months[2][this.getMonth()];
      if (patron.LANG == 'en_US') {
        return a + ' ' + this.format('d')
      }
      return this.format('d') + ' ' + a
    },
    getLocaleShortDayMonthYear: function () {
      if (patron.LANG == 'en_US') {
        return this.format('M/D/y')
      }
      return this.format('D.M.y')
    },
    diffYears: function (a, b, c) {
      if (isNaN(a)) {
        a = Date.parse(a);
        c = a.getDate();
        b = a.getMonth() + 1;
        a = a.getFullYear()
      }
      var d = this.getMonth() + 1;
      a = this.getFullYear() - a;
      if (b > d || c > this.getDate() && b == d) a--;
      return a
    }
  }); Date.setNow(window.time || window.TIME || Date.now()); setInterval(function () {
    Date.changeNow()
  }, 60000)
